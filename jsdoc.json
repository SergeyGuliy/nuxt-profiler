{
  "plugins": [
    "plugins/markdown",
    "plugins/summarize",
    "node_modules/jsdoc-vuejs"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "components",
      "functions",
      "store",
      "mixins",
      "./pages"
    ],
    "includePattern": "\\.(vue|js)$"
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./tutorial/documentation/",
    "tutorials": "tutorial"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
