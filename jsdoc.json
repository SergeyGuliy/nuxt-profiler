{
  "plugins": [
    "plugins/markdown",
    "plugins/summarize",
    "node_modules/jsdoc-vue"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "components",
      "functions",
      "layouts",
      "store",
      "middleware",
      "mixins",
      "pages",
      "plugins"
    ],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "recurse": true,
    "destination": "./documentation/"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
